{
  "author": "devJimmyboy",
  "browserslist": "last 2 chrome versions",
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24"
  },
  "description": "Template bundle for bundling with parcel.",
  "devDependencies": {
    "@parcel/resolver-glob": "^2.4.1",
    "@types/node": "^17.0.25",
    "autoprefixer": "^10.4.4",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.4",
    "postcss-url": "^10.1.3",
    "process": "^0.11.10",
    "typescript": "^4.6.3"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "homepage": "",
  "keywords": [
    "nodecg",
    "nodecg-bundle",
    "template",
    "parcel"
  ],
  "license": "MIT",
  "name": "controller-api",
  "nodecg": {
    "compatibleRange": "^1.4.0",
    "dashboardPanels": [
      {
        "file": "index.html",
        "headerColor": "#525F78",
        "name": "controller-api",
        "title": "NodeCG Api",
        "width": 2
      }
    ]
  },
  "scripts": {
    "build": "nodecg-bundler build -de",
    "build:dash": "parcel build src/dashboard/panel.html --dist-dir dashboard --public-url ./",
    "build:ext": "tsc --project ./src/extension",
    "dev": "run-p dev:*",
    "dev:dash": "parcel watch src/dashboard/panel.html --dist-dir dashboard --public-url ./",
    "dev:ext": "tsc --project ./src/extension --watch",
    "start": "npm run dev"
  },
  "version": "0.1.0"
}
