{
  "author": "devJimmyboy",
  "browserslist": "last 2 chrome versions",
  "dependencies": {
    "@chakra-ui/react": "^2.2.6",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "description": "Template bundle for bundling with parcel.",
  "devDependencies": {
    "@parcel/resolver-glob": "^2.7.0",
    "@types/node": "^18.7.2",
    "autoprefixer": "^10.4.8",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.10",
    "postcss-url": "^10.1.3",
    "process": "^0.11.10",
    "typescript": "^4.7.4"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "homepage": "",
  "keywords": [
    "nodecg",
    "nodecg-bundle",
    "template",
    "parcel"
  ],
  "license": "MIT",
  "name": "controller-api",
  "nodecg": {
    "compatibleRange": "^1.4.0",
    "dashboardPanels": [
      {
        "file": "index.html",
        "headerColor": "#525F78",
        "name": "controller-api",
        "title": "NodeCG Api",
        "width": 2
      }
    ]
  },
  "scripts": {
    "build": "nodecg-bundler build -de",
    "build:dash": "parcel build src/dashboard/panel.html --dist-dir dashboard --public-url ./",
    "build:ext": "tsc --project ./src/extension",
    "dev": "nodecg-bundler dev -de",
    "dev:dash": "parcel watch src/dashboard/panel.html --dist-dir dashboard --public-url ./",
    "dev:ext": "tsc --project ./src/extension --watch",
    "start": "npm run dev"
  },
  "version": "0.1.0"
}
