{
  "browserslist": "chrome >= 95",
  "dependencies": {
    "@eastdesire/jscolor": "^2.4.9",
    "@iconify/iconify": "^2.2.1",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "bad-words": "^3.0.4",
    "daisyui": "^2.24.0",
    "gsap": "^3.11.0",
    "jquery": "^3.6.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.4.0",
    "use-nodecg": "^0.4.1-1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.13",
    "autoprefixer": "^10.4.8",
    "nodecg-io-streamelements": "^0.2.0",
    "nodecg-types": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.10",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "api.js",
    "extension"
  ],
  "name": "simple-alerts",
  "nodecg": {
    "assetCategories": [
      {
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png",
          "webm"
        ],
        "name": "media-graphics",
        "title": "Graphics"
      },
      {
        "allowedTypes": [
          "mp3",
          "wav",
          "ogg"
        ],
        "name": "media-sounds",
        "title": "Sounds"
      }
    ],
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "file": "index.html",
        "headerColor": "#525F78",
        "name": "main-panel",
        "title": "Simple Alerts",
        "width": 4
      },
      {
        "file": "config.html",
        "headerColor": "#525F78",
        "name": "config-panel",
        "title": "Bad Words Config",
        "width": 2,
        "workspace": "config"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "height": 1080,
        "width": 1920
      }
    ]
  },
  "scripts": {
    "dev": "nodecg-bundler dev",
    "build": "nodecg-bundler build"
  },
  "version": "0.1.0"
}
