{
  "browserslist": "chrome >= 95",
  "dependencies": {
    "@eastdesire/jscolor": "^2.5.1",
    "@iconify/iconify": "^3.0.0",
    "animate.css": "^4.1.1",
    "axios": "^1.1.3",
    "bad-words": "^3.0.4",
    "daisyui": "^2.41.0",
    "gsap": "^3.11.3",
    "jquery": "^3.6.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.6.1",
    "uberduck-api": "^1.2.2",
    "use-nodecg": "^0.4.1-1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "autoprefixer": "^10.4.13",
    "nodecg-io-streamelements": "^0.2.0",
    "nodecg-types": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "api.js",
    "extension"
  ],
  "name": "simple-alerts",
  "nodecg": {
    "assetCategories": [
      {
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png",
          "webm"
        ],
        "name": "media-graphics",
        "title": "Graphics"
      },
      {
        "allowedTypes": [
          "mp3",
          "wav",
          "ogg"
        ],
        "name": "media-sounds",
        "title": "Sounds"
      }
    ],
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "file": "index.html",
        "headerColor": "#525F78",
        "name": "main-panel",
        "title": "Simple Alerts",
        "width": 4
      },
      {
        "file": "config.html",
        "headerColor": "#525F78",
        "name": "config-panel",
        "title": "Bad Words Config",
        "width": 2,
        "workspace": "config"
      },
      {
        "file": "ttsTest.html",
        "headerColor": "#525F78",
        "name": "tts-test",
        "title": "TTS Testing",
        "width": 3,
        "workspace": "config"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "height": 1080,
        "width": 1920
      }
    ]
  },
  "scripts": {
    "dev": "nodecg-bundler dev",
    "build": "nodecg-bundler build"
  },
  "version": "0.1.0"
}
