  "homepage": "",
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "license": "Apache-2.0",
  "name": "stream-starting",
  "nodecg": {
    "assetCategories": [
      {
        "allowedTypes": [
          "webm"
        ],
        "name": "starting-videos",
        "title": "Starting Soon Videos"
      },
      {
        "allowedTypes": [
          "webm"
        ],
        "name": "transition-videos",
        "title": "Starting Transition Videos"
      }
    ],
    "compatibleRange": "^1.6.1",
    "dashboardPanels": [
      {
        "file": "stream-starting.html",
        "fullbleed": true,
        "headerColor": "#44AA66",
        "name": "stream-starting",
        "title": "Stream Starting"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "height": 1080,
        "singleInstance": false,
        "width": 1920
      }
    ]
  },
  "repository": "https://github.com/devJimmyboy/stream-overlay.git",
  "scripts": {
    "build": "run-s build:*",
    "build:dash": "parcel build src/dashboard/stream-starting.html --dist-dir dashboard --public-url ./",
    "build:extension": "tsc -b src/extension",
    "build:graphics": "parcel build src/graphics/index.html --dist-dir graphics --public-url ./",
    "dev": "run-p dev:*",
    "dev:dash": "parcel watch src/dashboard/stream-starting.html --dist-dir dashboard --public-url ./",
    "dev:extension": "tsc -b -w src/extension",
    "dev:graphics": "parcel watch src/graphics/index.html --dist-dir graphics --public-url ./ --port 1235",
    "start": "npm run dev"
  },
  "version": "0.0.1"
}
