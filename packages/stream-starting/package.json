{
  "name": "stream-starting",
  "version": "0.0.1",
  "description": "",
  "homepage": "",
  "author": {
    "name": "devJimmyboy",
    "email": "devJimmyboy@gmail.com",
    "url": "https://jimmyboy.dev"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:*",
    "dev:dash": "parcel watch src/dashboard/stream-starting.html --dist-dir dashboard --public-url ./",
    "dev:graphics": "parcel watch src/graphics/index.html --dist-dir graphics --public-url ./ --port 1235",
    "build": "run-s build:*",
    "build:dash": "parcel build src/dashboard/stream-starting.html --dist-dir dashboard --public-url ./",
    "build:graphics": "parcel build src/graphics/index.html --dist-dir graphics --public-url ./"
  },
  "browserslist": "chrome >= 74",
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.6.1",
    "dashboardPanels": [
      {
        "name": "stream-starting",
        "title": "Stream Starting",
        "width": 4,
        "file": "stream-starting.html",
        "workspace": "Starting/Ending",
        "headerColor": "#44AA66"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "singleInstance": false,
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "starting-videos",
        "title": "Starting Soon Videos",
        "allowedTypes": [
          "webm"
        ]
      },
      {
        "name": "transition-videos",
        "title": "Starting Transition Videos",
        "allowedTypes": [
          "webm"
        ]
      }
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "daisyui": "^1.16.1",
    "gsap": "^3.8.0",
    "jquery": "^3.6.0",
    "nodecg-types": "^1.8.3",
    "normalize.css": "^8.0.1",
    "p5": "^1.4.0",
    "p5i": "^0.4.2",
    "pixi-filters": "^4.1.5",
    "pixi.js": "^6.2.0",
    "postcss-nesting": "^9.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.8",
    "@types/lodash": "^4.14.176",
    "@types/p5": "^1.3.1",
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^2.2.19"
  },
  "repository": "https://github.com/devJimmyboy/stream-overlay.git",
  "license": "Apache-2.0"
}
