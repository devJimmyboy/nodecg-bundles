{
  "name": "peepo-interactive",
  "version": "0.1.0",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "scripts": {
    "dev": "nodecg-bundler dev -dg",
    "build": "nodecg-bundler build -dg",
    "check": "tsc --noEmit"
  },
  "browserslist": "chrome >= 95",
  "nodecg": {
    "compatibleRange": "^1.6.1",
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "dashboardPanels": [
      {
        "name": "panel",
        "title": "Interactive Peepo",
        "width": 3,
        "file": "index.html",
        "headerColor": "#525F78"
      }
    ],
    "assetCategories": [
      {
        "name": "animations",
        "title": "Animations For Peepo",
        "allowedTypes": [
          "png",
          "json"
        ]
      }
    ]
  },
  "dependencies": {
    "@types/node": "^18.11.9",
    "gsap": "^3.11.3",
    "howler": "^2.2.3",
    "pixi.js": "^7.0.4"
  },
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "@types/p5": "^1.4.3",
    "@types/pixi.js": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.41.0",
    "nodecg": "^1.9.0",
    "nodecg-io-streamelements": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-url": "^10.1.3",
    "querystring-es3": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.4"
  }
}
