    "@twurple/pubsub": "^5.0.10",
    "@types/node": "^16.11.13",
    "daisyui": "^1.16.5",
    "dotenv": "^10.0.0",
    "framer-motion": "^5.5.1",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.31.0",
    "nanotimer": "^0.3.15",
    "nodecg": "^1.8.1",
    "nodecg-io-core": "^0.2.0",
    "nodecg-io-streamelements": "^0.2.0",
    "nodecg-io-twitch-addons": "^0.2.0",
    "nodecg-io-twitch-api": "^0.2.0",
    "nodecg-io-twitch-chat": "^0.2.0",
    "nodecg-io-twitch-pubsub": "^0.2.0",
    "nodecg-types": "^1.8.3",
    "postcss": "^8.4.5",
    "postcss-url": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.0",
    "react-select": "^5.2.1",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-widgets": "^5.5.1",
    "react-window": "^1.8.6",
    "tailwindcss": "^3.0.2",
    "use-nodecg": "^0.4.1-1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/resolver-glob": "^2.0.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/nanotimer": "^0.3.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "typescript": "^4.5.4"
  },
  "name": "twitch",
  "nodecg": {
    "bundleDependencies": {
      "nodecg-io-streamelements": "*",
      "nodecg-io-twitch-addons": "*",
      "nodecg-io-twitch-api": "*",
      "nodecg-io-twitch-chat": "*",
      "nodecg-io-twitch-pubsub": "*"
    },
    "compatibleRange": "^1.4.0",
    "dashboardPanels": [
      {
        "file": "panel.html",
        "fullbleed": true,
        "headerColor": "#9146FF",
        "name": "Twitch",
        "title": "Twitch Config"
      },
      {
        "file": "emoteTest.html",
        "headerColor": "#9146FF",
        "name": "EmoteTest",
        "title": "Emote Test",
        "width": "3",
        "workspace": "config"
      }
    ],
    "graphics": [
      {
        "file": "sub-goal.html",
        "height": 1080,
        "width": 1920
      }
    ]
  },
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:dash": "parcel build src/dashboard/*.html --dist-dir ./dashboard --public-url ./",
    "build:extension": "tsc -b src/extension",
    "build:graphics": "parcel build src/graphics/*.html --dist-dir ./graphics --public-url ./",
    "clean": "tsc -b --clean",
    "dev": "run-p dev:*",
    "dev:dash": "parcel watch src/dashboard/*.html --dist-dir ./dashboard --public-url ./",
    "dev:extension": "tsc -b -w src/extension",
    "dev:graphics": "parcel watch src/graphics/*.html --dist-dir ./graphics --public-url ./"
  },
  "version": "0.2.0"
}
