{
  "name": "obs",
  "version": "0.0.0",
  "description": "Connector holding Replicants for OBS Data like currentScene and more",
  "browserlists": [
    "chrome >= 95"
  ],
  "homepage": "",
  "author": {
    "name": "Jimmy Vrana",
    "email": "devJimmyboy@gmail.com",
    "url": "https://jimmyboy.dev"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "dashboardPanels": [
      {
        "file": "index.html",
        "name": "obs-graphics-config",
        "title": "OBS Graphics Config",
        "width": 3,
        "workspace": "config"
      }
    ],
    "assetCategories": [
      {
        "name": "logo",
        "title": "Logo Webm",
        "allowedTypes": [
          "webm"
        ]
      }
    ]
  },
  "scripts": {
    "dev": "nodecg-bundler dev",
    "build": "nodecg-bundler build",
    "clean": "tsc -b --clean"
  },
  "dependencies": {
    "@types/node": "^18.7.16",
    "gsap": "^3.11.1",
    "nodecg-types": "^1.9.0",
    "obs-websocket-js": "^5.0.1",
    "postcss": "^8.4.16",
    "typescript": "^4.8.3"
  },
  "repository": "https://github.com/devJimmyboy/stream-overlay.git",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "daisyui": "^2.25.0",
    "nodecg": "^1.9.0",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.1.10",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^3.1.8"
  }
}
