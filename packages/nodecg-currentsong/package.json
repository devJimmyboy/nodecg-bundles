{
  "name": "nodecg-currentsong",
  "version": "0.5.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "nodecg-bundler dev",
    "build": "nodecg-bundler build"
  },
  "homepage": "https://github.com/devJimmyboy/nodecg-currentsong",
  "authors": [
    "devJimmyboy"
  ],
  "browserslist": "chrome >= 95",
  "nodecg": {
    "compatibleRange": "^1.4.0",
    "description": "Connects to Last.fm's API & animates the currently playing song on change.",
    "dashboardPanels": [
      {
        "name": "current-song",
        "title": "Current Song Setup",
        "width": 3,
        "headerColor": "#2d4e8a",
        "workspace": "Config",
        "file": "index.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@toplast/lastfm": "^0.1.5",
    "@types/node": "^18.11.9",
    "axios": "^1.1.3",
    "eventemitter3": "^4.0.7",
    "fitty": "^2.3.6",
    "gsap": "^3.11.3",
    "jquery": "^3.6.1",
    "lastfm-typed": "^2.1.0",
    "materialize-css": "^1.0.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "nodecg-types": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.19",
    "postcss-import": "15.0.0",
    "postcss-nesting": "^10.2.0",
    "postcss-url": "^10.1.3",
    "tailwindcss": "3.2.4",
    "typed-emitter": "^2.1.0"
  }
}
